{
  "name": "forumi-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel --out-dir ./build . --source-maps --no-copy-ignored --ignore 'node_modules/**/*.js'",
    "build": "npm run clean && npm run build-server",
    "start": "dotenv -- cross-var pm2 start processes.json --no-daemon",
    "stop": "dotenv -- cross-var pm2 stop app",
    "kill": "dotenv -- cross-var pm2 kill",
    "prod": "dotenv -- cross-var pm2 start processes.json",
    "dev": "NODE_ENV=development nodemon --exec babel-node  --require dotenv/config app.js",
    "sequelize-auto-mysql": "sequelize-auto -o frameworks/database/mysql/models/ -d forumi -u root -x secret -h 127.0.0.1 --dialect mysql --port 33769"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.12.17",
    "@babel/runtime": "^7.12.18",
    "@godaddy/terminus": "^4.10.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cross-var": "^1.1.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "express": "~4.16.1",
    "helmet": "^5.1.0",
    "http": "^0.0.1-security",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.16",
    "pm2": "^5.2.0",
    "sanitize-html": "^2.7.0",
    "sequelize": "^6.20.0",
    "sequelize-auto": "^0.8.8",
    "swagger-autogen": "^2.21.2"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0"
  }
}
